{
    "version": 2,
    "name": "Maidreaminth-api",
    "builds": [{
        "src": "server.js",
        "use": "@now/node-server",
        "config": { "maxLambdaSize": "50mb" }
    }],
    "routes": [{
        "headers": {
            "Access-Control-Allow-Origin": "*",
            "Access-Control-Allow-Methods": "GET",
            "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
        },
        "src": "/.*",
        "dest": "/server.js"
    }]
}